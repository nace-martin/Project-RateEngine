regex:AKIA[0-9A-Z]{16}                               # AWS access key
regex:ASIA[0-9A-Z]{16}                               # AWS temp key
regex:aws(.{0,15})?(secret|access)_key              # generic AWS keys
regex:SECRET_ACCESS_KEY|AWS_SECRET_ACCESS_KEY
regex:AIza[0-9A-Za-z\-_]{35}                         # Google API key
regex:ghp_[A-Za-z0-9]{36}                            # GitHub classic token
regex:github_pat_[A-Za-z0-9_]{22,255}                # GitHub fine-grained PAT
regex:xox[baprs]-[A-Za-z0-9-]{10,48}                 # Slack token
regex:sk-[A-Za-z0-9]{20,99}                          # API-style tokens (Stripe, OpenAI, etc.)
regex:Authorization:\s*Bearer\s+[A-Za-z0-9\._-]{20,} # Bearer tokens
regex:-----BEGIN (?:RSA |DSA |EC )?PRIVATE KEY-----  # PEM private keys
regex:-----BEGIN PRIVATE KEY-----
regex:id_rsa(\.pub)?$
regex:(?i)password\s*[:=]\s*[^,\s'"]{6,}            # password=... style
regex:(?i)PGPASSWORD=.+                              # Postgres passwords
regex:(?i)DATABASE_URL=.+                            # Database connection strings
regex:(?i)client_secret                              # OAuth client secrets
regex:(?i)service[-_ ]?account.*\.json               # GCP service account JSON
regex:(?i)firebase.*adminsdk.*\.json                 # Firebase admin SDK JSON
